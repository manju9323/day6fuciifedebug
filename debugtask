//Fix the code to Sum of the digits present in the number
//Code:
let n = "123";
function add(n)
{
let sum =0;
for(var i=0;i<n.length;i++){
 sum+=Number(n[i])
 }
 return sum;
}
val=add(n);
console.log(val)
